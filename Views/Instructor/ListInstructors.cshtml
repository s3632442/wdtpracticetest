@model InstructorsViewModel

<!DOCTYPE html>
<html>
<head>
    <title>List of Instructors</title>
</head>
<body>
    <h1>List of Instructors</h1>
    
    <table>
        <tr>
            <th>Instructor ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Hire Date</th>
            <th>...</th> <!-- Add more columns as needed -->
        </tr>
        
        @foreach (var instructor in Model.Instructors)
        {
            <tr>
                <td>@instructor.ID</td>
                <td>@instructor.FirstName</td>
                <td>@instructor.LastName</td>
                <td>@instructor.HireDate</td>
                <td>...</td> <!-- Add more columns as needed -->
            </tr>
        }
    </table>
</body>
</html>
